<%= turbo_frame_tag dom_id(@pr_signet, :gh_pull_requests) do %>
    <% if @pr_signet.gh_pull_requests.any? %>
      <%= render partial: 'pull_requests/pull_request', collection: @pr_signet.gh_pull_requests, cached: true %>
    <% else %>
      <div class="text-center pt-3">
        <p class="text-muted">Empty</p>
      </div>
    <% end %>
<% end %>
